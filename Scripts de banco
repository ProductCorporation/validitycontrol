CREATE TABLE gerente(
    ger_cod int PRIMARY KEY,
    ger_nome varchar2(30)
);

CREATE TABLE funcionario(
    func_cod int PRIMARY KEY,
    func_nome varchar2(30),
    func_idade int,
    func_sexo varchar2(20)
);

CREATE TABLE Produto(
    prod_cod int PRIMARY KEY,
    prod_nome varchar2(30),
    prod_categoria varchar2(30),
    prod_vencimento varchar2(15)
);

CREATE TABLE Fornecedor(
    for_cod int PRIMARY KEY,
    for_local varchar2(40),
    for_nome varchar2(30),
    for_telefone varchar2(15)
);

CREATE TABLE Empresa(
    empr_cod int PRIMARY KEY,
    empr_nome varchar2(30),
    empr_local varchar2(50),
    empr_telefone varchar2(15)
);

